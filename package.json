{
  "name": "Saas1000",
  "version": "2.4.0",
  "description": "Saas 1000 project",
  "scripts": {
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:client": "webpack-dev-server --progress --colors --inline --env.dev",
    "start:server": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=development nodemon index.js",
    "start:prod": "cross-env NODE_ENV=production node dist/server.bundle.js",
    "bs": "npm run clean && npm run build:client && npm run build:server && npm run start:prod",
    "build:client": "webpack -p --progress --env.prod",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack.config.server.js",
    "clean": "rimraf dist",
    "slate": "rimraf node_modules && npm install",
    "lint": "eslint client server"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "license": "MIT",
  "dependencies": {
